    "@sentry/nextjs": "^7.101.1",
    "@vercel/speed-insights": "^1.0.10",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.4",
    "isomorphic-dompurify": "^2.3.0",
    "next": "14.1.0",
    "openai": "^4.28.0",
    "prisma": "^5.10.2",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.70.0",
    "semver": "^7.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
